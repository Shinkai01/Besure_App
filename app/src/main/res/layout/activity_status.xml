<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bluestats"
    >
    <TextView
        android:id="@+id/titleHome"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="44dp"
        android:fontFamily="@font/orangetitle"
        android:text="ESURE"
        android:textColor="#03A9F4"
        android:textSize="60sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.utils.widget.ImageFilterView
        android:id="@+id/imageFilterView"
        android:layout_width="338dp"
        android:layout_height="575dp"
        android:scaleType="fitXY"
        android:tint="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titleHome"
        app:layout_constraintVertical_bias="0.375"
        app:srcCompat="@xml/rectangle" />

    <TextView
        android:id="@+id/textView401"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="508dp"
        android:fontFamily="@font/blue"
        android:text="Hello,"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageFilterView"
        app:layout_constraintEnd_toStartOf="@+id/textView3"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imageFilterView" />

    <TextView
        android:id="@+id/thisis"
        android:layout_width="238dp"
        android:layout_height="33dp"
        android:fontFamily="@font/blue"
        android:text="This is your scooter current Status."
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageFilterView"
        app:layout_constraintEnd_toEndOf="@+id/imageFilterView"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="@+id/imageFilterView"
        app:layout_constraintTop_toTopOf="@+id/imageFilterView"
        app:layout_constraintVertical_bias="0.149" />



    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/blue"
        android:text="!"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageFilterView"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/imageFilterView" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/blue"
        android:text="User"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageFilterView"
        app:layout_constraintEnd_toEndOf="@+id/imageFilterView"
        app:layout_constraintHorizontal_bias="0.294"
        app:layout_constraintStart_toStartOf="@+id/imageFilterView"
        app:layout_constraintTop_toTopOf="@+id/imageFilterView"
        app:layout_constraintVertical_bias="0.052" />

    <RelativeLayout
        android:layout_width="301dp"
        android:layout_height="440dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageFilterView"
        app:layout_constraintEnd_toEndOf="@+id/imageFilterView"
        app:layout_constraintStart_toStartOf="@+id/imageFilterView">

        <androidx.constraintlayout.utils.widget.ImageFilterView
            android:id="@+id/backgroundon"
            android:layout_width="match_parent"
            android:layout_height="94dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="0dp"
            android:layout_marginEnd="0dp"
            android:layout_marginBottom="346dp"
            android:scaleType="fitXY"
            android:tint="#FF03A9F4"
            app:srcCompat="@xml/rectangle2" />

        <androidx.constraintlayout.utils.widget.ImageFilterView
            android:id="@+id/backgroundtimer"
            android:layout_width="183dp"
            android:layout_height="130dp"
            android:layout_above="@+id/backgroundlocation"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="0dp"
            android:layout_marginBottom="23dp"
            android:scaleType="fitXY"
            android:tint="#FF03A9F4"
            app:srcCompat="@xml/rectangle2"
            tools:layout_editor_absoluteX="172dp"
            tools:layout_editor_absoluteY="335dp" />

        <androidx.constraintlayout.utils.widget.ImageFilterView
            android:id="@+id/backgroundlocation"
            android:layout_width="match_parent"
            android:layout_height="134dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="0dp"
            android:layout_marginEnd="0dp"
            android:layout_marginBottom="47dp"
            android:scaleType="fitXY"
            android:tint="#FF03A9F4"
            app:srcCompat="@xml/rectangle2"
            tools:layout_editor_absoluteX="58dp"
            tools:layout_editor_absoluteY="485dp" />

        <androidx.constraintlayout.utils.widget.ImageFilterView
            android:id="@+id/backgroundmema"
            android:layout_width="107dp"
            android:layout_height="129dp"
            android:layout_above="@+id/backgroundlocation"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="194dp"
            android:layout_marginBottom="23dp"
            android:scaleType="fitXY"
            android:tint="#FF03A9F4"
            app:srcCompat="@xml/rectangle2"
            tools:layout_editor_absoluteX="58dp"
            tools:layout_editor_absoluteY="334dp" />

        <TextView
            android:id="@+id/sulatlocation"
            android:layout_width="wrap_content"
            android:layout_height="31dp"
            android:layout_alignStart="@+id/backgroundlocation"
            android:layout_alignTop="@+id/backgroundlocation"
            android:layout_alignEnd="@+id/backgroundlocation"
            android:layout_alignBottom="@+id/backgroundlocation"
            android:layout_marginStart="14dp"
            android:layout_marginTop="17dp"
            android:layout_marginEnd="41dp"
            android:layout_marginBottom="89dp"
            android:fontFamily="@font/blue"
            android:text="location:"
            android:textColor="#FEFEFE"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintVertical_bias="0.699" />

        <TextView
            android:id="@+id/timersulat"
            android:layout_width="128dp"
            android:layout_height="35dp"
            android:layout_alignStart="@+id/backgroundtimer"
            android:layout_alignTop="@+id/backgroundtimer"
            android:layout_alignEnd="@+id/backgroundtimer"
            android:layout_alignBottom="@+id/backgroundtimer"
            android:layout_marginStart="13dp"
            android:layout_marginTop="27dp"
            android:layout_marginEnd="36dp"
            android:layout_marginBottom="70dp"
            android:fontFamily="@font/blue"
            android:text="See Location: "
            android:textColor="#FEFEFE"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintVertical_bias="0.699" />

        <TextView
            android:id="@+id/onoffsulat"
            android:layout_width="wrap_content"
            android:layout_height="47dp"
            android:layout_alignStart="@+id/backgroundon"
            android:layout_alignTop="@+id/backgroundon"
            android:layout_alignEnd="@+id/backgroundon"
            android:layout_alignBottom="@+id/backgroundon"
            android:layout_marginStart="38dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="142dp"
            android:layout_marginBottom="39dp"
            android:fontFamily="@font/blue"
            android:text="Status:"
            android:textColor="#FEFEFE"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintVertical_bias="0.699" />

        <TextView
            android:id="@+id/onoff"
            android:layout_width="67dp"
            android:layout_height="31dp"
            android:layout_alignStart="@+id/backgroundon"
            android:layout_alignTop="@+id/backgroundon"
            android:layout_alignEnd="@+id/backgroundon"
            android:layout_alignBottom="@+id/backgroundon"
            android:layout_marginStart="115dp"
            android:layout_marginTop="29dp"
            android:layout_marginEnd="115dp"
            android:layout_marginBottom="10dp"
            android:fontFamily="@font/blue"
            android:gravity="center"
            android:text="Invalid"
            android:textColor="#FEFEFE"
            android:textSize="20sp"
            app:layout_constraintVertical_bias="0.699" />

        <Button
            android:id="@+id/timer"
            android:layout_width="135dp"
            android:layout_height="45dp"
            android:layout_alignStart="@+id/backgroundtimer"
            android:layout_alignTop="@+id/backgroundtimer"
            android:layout_alignEnd="@+id/backgroundtimer"
            android:layout_alignBottom="@+id/backgroundtimer"
            android:layout_marginStart="23dp"
            android:layout_marginTop="65dp"
            android:layout_marginEnd="25dp"
            android:layout_marginBottom="20dp"
            android:backgroundTint="#1661B3"
            android:fontFamily="@font/blue"
            android:gravity="center"
            android:text="Click Here"
            android:textColor="#FEFEFE"
            android:textSize="20sp"
            app:layout_constraintVertical_bias="0.699" />

        <TextView
            android:id="@+id/locationlat"
            android:layout_width="wrap_content"
            android:layout_height="34dp"
            android:layout_alignStart="@+id/backgroundlocation"
            android:layout_alignTop="@+id/backgroundlocation"
            android:layout_alignEnd="@+id/backgroundlocation"
            android:layout_alignBottom="@+id/backgroundlocation"
            android:layout_marginStart="29dp"
            android:layout_marginTop="91dp"
            android:layout_marginEnd="26dp"
            android:layout_marginBottom="12dp"
            android:fontFamily="@font/blue"
            android:gravity="center"
            android:text="Invalid"
            android:textColor="#FEFEFE"
            android:textSize="20sp"
            app:layout_constraintVertical_bias="0.699" />

        <TextView
            android:id="@+id/locationlong"
            android:layout_width="wrap_content"
            android:layout_height="37dp"
            android:layout_alignStart="@+id/backgroundlocation"
            android:layout_alignTop="@+id/backgroundlocation"
            android:layout_alignEnd="@+id/backgroundlocation"
            android:layout_alignBottom="@+id/backgroundlocation"
            android:layout_marginStart="29dp"
            android:layout_marginTop="51dp"
            android:layout_marginEnd="26dp"
            android:layout_marginBottom="49dp"
            android:fontFamily="@font/blue"
            android:gravity="center"
            android:text="Invalid"
            android:textColor="#FEFEFE"
            android:textSize="20sp"
            app:layout_constraintVertical_bias="0.699" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="100dp"
            android:layout_height="123dp"
            android:layout_alignStart="@+id/backgroundmema"
            android:layout_alignTop="@+id/backgroundmema"
            android:layout_alignEnd="@+id/backgroundmema"
            android:layout_alignBottom="@+id/backgroundmema"
            android:layout_marginStart="4dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="3dp"
            android:layout_marginBottom="1dp"
            app:srcCompat="@drawable/escootericonhehe"
            app:tint="#FFFFFF" />
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>